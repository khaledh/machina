// Numeric literal bases and digit grouping

fn main() {
    let dec = 42;
    let bin = 0b1010_0110;
    let oct = 0o52;
    let hex = 0x2a;
    let grouped = 1_000_000;

    print("dec = ");
    println(dec);
    print("bin = ");
    println(bin);
    print("oct = ");
    println(oct);
    print("hex = ");
    println(hex);
    print("grouped = ");
    println(grouped);
}
