// Arithmetic operators

fn main() {
    let a = 20;
    let b = 6;

    let add = a + b;
    let sub = a - b;
    let mul = a * b;
    let div = a / b;
    let rem = a % b;

    print("add = ");
    println(add);
    print("sub = ");
    println(sub);
    print("mul = ");
    println(mul);
    print("div = ");
    println(div);
    print("rem = ");
    println(rem);
}
